{
  "name": "@fluentify/fluentify",
  "version": "0.1.0",
  "description": "Fluent design system components based framework for Vue.js",
  "author": "Alexander Wennerstr√∏m <alexanderw0310@gmail.com>",
  "scripts": {
    "build": "vue-cli-service build",
    "serve": "vue-cli-service serve",
    "test": "cross-env BABEL_ENV=test karma start test/karma.conf.js --single-run",
    "clean": "rimraf dist",
    "coverage": "cross-env BABEL_ENV=test karma start test/karma.conf.js",
    "lint:css": "stylelint src/**/*.{vue,css,scss}",
    "lint:js": "npx eslint --ext js --ext jsx --ext vue src src/**/*.spec.js build",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:staged": "lint-staged",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "storybook:build": "build-storybook",
    "storybook:serve": "start-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/vue": "^5.2.5",
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "file-loader": "^4.2.0",
    "lint-staged": "^8.2.1",
    "node-sass": "^4.12.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.7.1",
    "sass-loader": "^7.3.1",
    "stylelint": "^11.1.1",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "browser": "dist/fluentify.js",
  "bugs": {
    "url": "https://github.com/FluentifyJs/fluentify/issues"
  },
  "dllPlugin": {
    "name": "vuePluginTemplateDeps",
    "include": [
      "mocha/mocha.js",
      "!!style-loader!!css-loader!mocha-css",
      "html-entities",
      "vue/dist/vue.js",
      "chai",
      "core-js/library",
      "url",
      "sockjs-client",
      "vue-style-loader/lib/addStylesClient.js",
      "events",
      "ansi-html",
      "style-loader/lib/addStyles.js"
    ],
    "loaders": [
      "style-loader",
      {
        "loader": "css-loader",
        "options": {
          "importLoaders": 1
        }
      },
      {
        "loader": "sass-loader",
        "options": {
          "importLoaders": 1
        }
      }
    ]
  },
  "files": [
    "/build/",
    "/dist/",
    "/src/"
  ],
  "homepage": "https://github.com/FluentifyJs/fluentify#readme",
  "keywords": [
    "vue",
    "vuejs",
    "fluent",
    "design",
    "framework",
    "components"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{vue,jsx,js}": [
      "npm run lint:js"
    ],
    "*.{vue,css}": [
      "npx stylefmt",
      "npm run lint:css"
    ]
  },
  "main": "dist/fluentify.common.js",
  "module": "dist/fluentify.esm.js",
  "pre-commit": "lint:staged",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FluentifyJs/fluentify.git"
  },
  "style": "dist/fluentify.css",
  "unpkg": "dist/fluentify.js",
  "dependencies": {
    "vue": "^2.6.10"
  }
}
